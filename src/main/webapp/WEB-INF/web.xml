<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UsersController</servlet-name>
  	<display-name>UsersController</display-name>
  	<description></description>
  	<servlet-class>com.online_appoinment_web.controller.UsersController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.online_appoinment_web.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AppoinmentController</servlet-name>
  	<display-name>AppoinmentController</display-name>
  	<description></description>
  	<servlet-class>com.online_appoinment_web.controller.AppoinmentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ConsultantController</servlet-name>
  	<display-name>ConsultantController</display-name>
  	<description></description>
  	<servlet-class>com.online_appoinment_web.controller.ConsultantController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UsersController</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AppoinmentController</servlet-name>
  	<url-pattern>/appoinment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ConsultantController</servlet-name>
  	<url-pattern>/consultant</url-pattern>
  </servlet-mapping>
</web-app>
